      #fh5co-started.fh5co-bg-dark
        .overlay
        .container
          .row.animate-box
            .col-md-8.col-md-offset-2.text-center.fh5co-heading
              %h2 Hire Me!
              %p Facilis ipsum reprehenderit nemo molestias. Aut cum mollitia reprehenderit. Eos cumque dicta adipisci architecto culpa amet.
              %p
                %a.btn.btn-default.btn-lg{:href => "#"} Contact Us
      #fh5co-consult
        .video.fh5co-video{:style => "background-image: image_tag(cover_bg_1.jpg);"}
          .overlay
        .choose.animate-box
          %h2 Contact
          %form{:action => "#"}
            .row.form-group
              .col-md-6
                %input#fname.form-control{:placeholder => "Your firstname", :type => "text"}/
            .row.form-group
              .col-md-6
                %input#lname.form-control{:placeholder => "Your lastname", :type => "text"}/
            .row.form-group
              .col-md-12
                %input#email.form-control{:placeholder => "Your email address", :type => "text"}/
            .row.form-group
              .col-md-12
                %input#subject.form-control{:placeholder => "Your subject of this message", :type => "text"}/
            .row.form-group
              .col-md-12
                %textarea#message.form-control{:cols => "30", :name => "message", :placeholder => "Say something about us", :rows => "10"}
            .form-group
              %input.btn.btn-primary{:type => "submit", :value => "Send Message"}/
